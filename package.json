{
  "name": "udplistener",
  "version": "1.0.4",
  "description": "Site servey kit to recive data via UDP datagram",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack-all": "electron-builder -mwl",
    "pack-mw": "electron-builder -mw",
    "pack-win": "electron-builder -w",
    "pack-mac": "electron-builder -m",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Shashank Padwal",
  "license": "ISC",
  "dependencies": {
    "detect-ssid": "^1.0.3",
    "ip": "^1.1.5"
  },
  "devDependencies": {
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1"
  },
  "build": {
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
