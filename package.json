{
  "name": "site-survey-kit",
  "version": "1.0.0",
  "description": "Site servey kit to recive data via UDP datagram",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64  --prune=true --out=release-builds",
    "package-win": "electron-packager . UDP_listner --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"UDP_listner\"",
    "package-linux": "electron-packager . UDP_listner --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Shashank Padwal",
  "license": "ISC",
  "dependencies": {
    "electron": "^10.1.3",
    "ip": "^1.1.5"
  }
}
